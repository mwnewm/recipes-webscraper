{
  "manifest_version": 2,
  "name": "Recipe WebScraper",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js" : [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "ingredient_scrape.js"
    ],
    "persistent": false
  }
}
